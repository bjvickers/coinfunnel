    "jsonwebtoken": "8.0.1",
    "lodash": "4.17.5",
    "mongoose": "4.11.12",
    "node-cron": "1.2.1",
    "pagination": "0.4.4",
    "popper.js": "1.12.9",
    "request": "2.85.0",
    "sanitizer": "0.1.3",
    "serve-favicon": "2.4.5",
