{
  "name": "coinfunnel-server",
  "version": "0.0.1",
  "description": "",
  "main": "./src/bin/start.js",
  "scripts": {
    "lint": "./node_modules/.bin/standard \"src/**/*.js\"",
    "fix-style": "./node_modules/.bin/standard --fix \"src/**/*.js\"",
    "build-all-assets": "./node_modules/.bin/gulp",
    "build-js": "./node_modules/.bin/gulp buildJS",
    "build-css": "./node_modules/.bin/gulp buildCSS",
    "test-real-services": "NODE_ENV=test-real-services MONGO_DB_NAME=test ELASTICSEARCH_INDEX=test ./node_modules/.bin/cucumber-js ./tests --tags \"not @wip\" --exit",
    "test": "NODE_ENV=test ./node_modules/.bin/cucumber-js ./tests --tags \"not @wip\" --exit",
    "test-coverage": "./node_modules/.bin/nyc npm test",
    "coveralls": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "debug": "./node_modules/.bin/forever --watch --watchDirectory=/app --watchIgnore=/app/node_modules -l forever-log.log ./src/bin/start.js",
    "start": "./node_modules/.bin/forever -l forever-log.log ./src/bin/start.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "app-root-path": "2.0.1",
